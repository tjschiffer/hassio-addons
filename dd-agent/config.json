{
  "name": "Datadog Agent",
  "version": "0.9",
  "slug": "datadog-agent",
  "description": "Agent to stream your Home Assistant metrics to Datadog",
  "url": "https://github.com/DataDog/dd-agent",
  "startup": "system",
  "boot": "auto",
  "ports": {
    "8125/udp": 8125
  },
  "host_network": true,
  "environment": {
    "NON_LOCAL_TRAFFIC": false
  },
  "options": {
    "DD_API_KEY": ""
  },
  "schema": {
    "DD_API_KEY": "str"
  }
}
